{
  "name": "Unsplashed new Tab",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Enjoy a new unsplashed image at each new tab",
  "homepage_url": "https://github.com/quentin-sommer/unsplashed-new-tab",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.js",
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
