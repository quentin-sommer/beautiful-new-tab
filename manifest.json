{
  "name": "Beautiful new Tab",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Enjoy a new featured Unsplash photo with or without a personalized welcome message when you open a new tab in Google Chrome.",
  "homepage_url": "https://github.com/quentin-sommer/beautiful-new-tab",
  "icons": {
    "16": "icons/bntab16.png",
    "48": "icons/bntab48.png",
    "128": "icons/bntab128.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "https://api.unsplash.com/*",
    "storage"
  ]
}
